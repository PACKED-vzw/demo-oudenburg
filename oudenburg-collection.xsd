<?xml version="1.0" encoding="UTF-8"?>
<xs:schema 
  xmlns:xs="http://www.w3.org/2001/XMLSchema"
  targetNamespace="http://example.org/oudenburg-dc"
  xmlns="http://example.org/oudenburg-dc"
  elementFormDefault="qualified">
  <xs:element name="descriptionSet">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="description" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="description">
    <xs:complexType>
      <xs:sequence>
        <xs:any maxOccurs="unbounded"></xs:any>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>
